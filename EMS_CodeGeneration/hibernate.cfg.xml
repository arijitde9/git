<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-configuration PUBLIC
"-//Hibernate/Hibernate Configuration DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
    <session-factory>
        <property name="hibernate.connection.driver_class">com.mysql.jdbc.Driver</property>
        <property name="hibernate.connection.password">root</property>
        <property name="hibernate.connection.url">jdbc:mysql://localhost:3306/ems</property>
        <property name="hibernate.connection.username">root</property>
        <property name="hibernate.dialect">org.hibernate.dialect.MySQLDialect</property>
        <property name="hibernate.search.autoregister_listeners">false</property>
        <mapping class="com.ems.app.files.EmsUserAttendance" />
        <mapping class="com.ems.app.files.EmsDesigMaster" />
        <mapping class="com.ems.app.files.EmsUserPwd" />
        <mapping class="com.ems.app.files.EmsUserMaster" />
        <mapping class="com.ems.app.files.EmsUserDesig" />
        <mapping class="com.ems.app.files.EmsLeaveType" />
        <mapping class="com.ems.app.files.EmsMasterKey" />
        <mapping class="com.ems.app.files.EmsStateMaster" />
        <mapping class="com.ems.app.files.EmsUserLogin" />
        <mapping class="com.ems.app.files.EmsUserAttnDetails" />
        <mapping class="com.ems.app.files.EmsCountryMaster" />
        <mapping class="com.ems.app.files.EmsUserLeaveDetails" />
    </session-factory>
</hibernate-configuration>
